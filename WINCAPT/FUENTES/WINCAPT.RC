			MENUITEM "&Normal", IDM_DIODO
			MENUITEM "&Zener", IDM_DIODOZ
			MENUITEM "&Varicap", IDM_DIODOV
		END

		POPUP "&Transistor"
		BEGIN
			MENUITEM "&Bipolar", IDM_BIPOLAR
			MENUITEM "&JFET", IDM_JFET
			MENUITEM "&MOSFET", IDM_MOSFET
		END

		MENUITEM "B&atería", IDM_BATERIA
