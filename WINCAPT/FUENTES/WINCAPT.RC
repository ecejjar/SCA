		MENUITEM "&Vertical", IDM_VERT, CHECKED
		MENUITEM "&Espejo", IDM_ESPEJO, CHECKED
		MENUITEM "Re&jilla", IDM_REJILLA, CHECKED
	END

	POPUP "Biblio&teca"
	BEGIN
		MENUITEM "&Crear Biblioteca...", IDM_CREABIBL
