#include <windows.h>
#include <wincapt.h>

capturador ICON   c:\borlandc\proyecto\iconos\wincapt.ico
capturador BITMAP DISCARDABLE c:\borlandc\proyecto\bitmaps\wincapt.bmp

capturador MENU 
BEGIN
	POPUP "&Circuito"
	BEGIN
		MENUITEM "&Nuevo", IDM_EMPEZAR
